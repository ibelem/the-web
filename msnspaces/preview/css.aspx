//from the contact card
    .ContactCard SPAN
    {
        white-space:nowrap;
    }
    .ContactCard SPAN.padding { width:0px }
    .ContactCard IMG.tile { float: left; height: 96px; width: 96px; margin-left: 8px; }
    .ContactCard IMG
    {
        border: 0px;
    }
    .ContactCard IMG.ContactCardThumbnailNormal
    {
    }
    .ContactCard IMG.ContactCardThumbnailSelect
    {
        filter: progid:DXImageTransform.Microsoft.dropShadow(Color=999999,offX=2,offY=2,positive=true);    
    }
    .ContactCard TABLE { table-layout:fixed; }
    .ContactCard P { height: 2px }
    .ContactCard IMG.Highlighted
    {
        border: 1px;
    }
    .ContactCard IMG.Normal
    {
        border: 0px;
    }
    .ContactCard A
    {
        text-decoration: none;
    }
    .ContactCard A:hover
    {
        text-decoration: underline;
        color: #000075;
    }
    .ContactCard A.MySpace
    {
        color: #000075;
    }
    .ContactCard TABLE.Main 
    { 
        background-color: #FBB088;
        color: #000000;
        font-family: ;
        filter: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#FBB088,endColorStr=#FBB088);
        border-left: #D86F38 1px solid;
        border-right: #D86F38 1px solid;                        
    }
    .ContactCard TABLE.MainContent
    {
        background-color: #FFFFFF;
        color: #000075;
        font-family: ;
        background-image: url('../Style/OrangeDaisies/OrangeDaisies_card_bkgd.gif');
        height: 158px;
        border-left: #D86F38 1px solid;
        border-right: #D86F38 1px solid;
        
    }
    .ContactCard A.FooterA
    {
        color: #000075;     
    }
    .ContactCard TABLE.Footer
    {
        background-color: #FBB08B;
        color: #000075;
        font-family: ;      
        filter: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#FBB08B,endColorStr=#FADED1);
        border-left: #D86F38 1px solid;
        border-right: #D86F38 1px solid;                        
    }
    .ContactCard TD
    {
        background-image: none;
        background-color: transparent;
        text-overflow:ellipsis;
        overflow:hidden;                    
    }   
    .ContactCard TR.Profile
    {
         height: 16px;
    }
    .ContactCard TR.MySpaceRow
    {
        height: 5px;
    }
    .ContactCard TR.EmptyRow
    {
        height: 10px;
    }
    .ContactCard TR.MoreLinkRow
    {
    }
    .ContactCard TR.PhotoCaptionRow
    {
        height: 12px;
    }
    .ContactCard TD.SummaryCell
    {
        text-overflow:ellipsis;
        overflow:hidden;

    }
    .ContactCard IMG.DisplayPicture
    {
        border: #D86F38 1px solid;
    }
